{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/frontend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next build",
        "cwd": "apps/frontend"
      },
      "configurations": {
        "production": {}
      },
      "outputs": ["{projectRoot}/.next"]
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "next dev",
        "cwd": "apps/frontend"
      },
      "configurations": {
        "production": {
          "command": "next start",
          "cwd": "apps/frontend"
        }
      }
    }
  },
  "tags": []
}
